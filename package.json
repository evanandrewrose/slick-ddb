{
  "name": "slick-dynamodb",
  "version": "1.0.1",
  "main": "./dist/slick-dynamodb.cjs",
  "module": "./dist/slick-dynamodb.mjs",
  "exports": {
    ".": {
      "require": "./dist/slick-dynamodb.cjs",
      "import": "./dist/slick-dynamodb.mjs"
    }
  },
  "types": "./dist/slick-dynamodb.d.ts",
  "scripts": {
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test:watch": "jest --config --watch jest.config.ts --max-workers=1",
    "test": "jest --config jest.config.ts",
    "test:coverage": "jest --runInBand --coverage --config jest.config.ts",
    "lint:scripts": "eslint . --ext .ts",
    "format:scripts": "prettier . --write",
    "format": "npm run format:scripts"
  },
  "peerDependencies": {
    "aws-sdk": "^2.1131.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "dts-bundle-generator": "^6.5.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  }
}
